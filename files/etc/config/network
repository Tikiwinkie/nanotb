config interface 'loopback'
    option ifname    'lo'
    option proto     'static'
    option ipaddr    '127.0.0.1'
    option netmask   '255.0.0.0'
    option multipath 'off'

config interface 'lan'
    option ifname    'eth0'
    option proto     'dhcp'
    option multipath 'on'

config interface 'mud'
    option ifname     'tun0'
    option proto      'static'
    option ipaddr     '10.166.179.2'
    option gateway    '10.166.179.1'
    option ip4table   'vpn'
    option netmask    '255.255.255.0'
    option multipath  'off'
    option txqueuelen '1000'
