
config icmp 'icmp1'
	option host '82.200.209.194'
	option timeout '2'
	option interface 'if1'
	option retry '2'

config icmp 'icmp2'
	option host '8.8.8.8'
	option timeout '2'
	option interface 'if2'
	option retry '2'

config dns 'dns1'
	option host '208.67.222.220'
	option timeout '2'
	option interface 'if1'
	option domain 'myip.opendns.com'
	option retry '2'

config dns 'dns2'
	option host '208.67.222.220'
	option timeout '2'
	option interface 'if2'
	option domain 'myip.opendns.com'
	option retry '2'

config tcp_curl 'curl1'
	option host 'ipaddr.ovh'
	option timeout '2'
	option interface 'if1'
	option retry '2'

config tcp_curl 'curl2'
	option host 'ipaddr.ovh'
	option timeout '2'
	option interface 'if2'
	option retry '2'

